{
  "name": "bear-eatery-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:js": "browserify src/js/index.js -d -t babelify -o build/bundle.js",
    "watch:js": "watchify src/js/index.js -d -v -t babelify -o build/bundle.js",
    "build:scss": "node-sass src/scss/styles.scss --output build --source-map-embed --source-map-contents",
    "watch:scss": "npm run build:scss -- --watch",
    "build": "npm run build:js && npm run build:scss",
    "serve": "npm run build; npm run watch:js -- & > /dev/null 2>&1; npm run watch:scss -- & > /dev/null 2>&1; lite-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avaniklsjoshi/bear-eatery-js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/avaniklsjoshi/bear-eatery-js/issues"
  },
  "homepage": "https://github.com/avaniklsjoshi/bear-eatery-js#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "bulma": "^0.5.0"
  }
}
